{
  "name": "iexec-explorer",
  "private": true,
  "version": "1.3.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "format": "prettier --write src",
    "check-format": "prettier --check src",
    "preview": "vite preview",
    "codegen": "npm run codegen:poco",
